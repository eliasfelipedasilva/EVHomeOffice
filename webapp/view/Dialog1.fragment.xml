<core:FragmentDefinition xmlns:core="sap.ui.core" controllerName="com.sap.build.standard.vai.controller.Dialog1" xmlns="sap.m">
    <Dialog horizontalScrolling="true" showHeader="true" state="None" stretch="false" title="Atenção" type="Standard" verticalScrolling="true">
        <content>
            <Label text="Tem certeza que deseja  finalizar a Avaliação?" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
        </content>
        <beginButton/>
        <buttons>
            <Button text="OK" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress"/>
            <Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress1"/>
        </buttons>
        <customHeader/>
        <endButton/>
        <subHeader/>
    </Dialog>
</core:FragmentDefinition>
